              <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <!--<li><a href="#">Blog</a></li>-->
                            <li><a href="/user/dashboard">Resources</a></li>
                            <!--<li><a href="#">Photography</a></li>-->
                            <li role="separator" class="divider"></li>
