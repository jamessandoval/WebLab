                <ul class="nav navbar-nav">
                    {{#if user}}
                    <li><a href="/user/addpost">Add a Post</a></li>
                    <li><a href="/user/dashboard">Dashboard</a></li>
                    <li><a href="/user/controlpanel">Control Panel</a></li>
                    <li><a href="/user/logout">Logout</a></li>
                    {{/if}}  
                </ul>
            </div>
            {{/if}}
            <!--/.nav-collapse -->
